an1 = an1 * we1
an2 = an2 * we2
an3 = an3 * we3
an1 = an1 * we4
an2 = an2 * we5
an3 = an3 * we6
output = an1 + an2 + an3
print(output)
anweightdata = (we1, we2,  we3, we4, we5, we6)
if output >= 1.5:
    output = 2
    print(output)
elif output >= 0.5:
      output = 1
      print(output) 
else:
    output = 0
    print(output)

anweightdata = (we1, we2,  we3, we4, we5, we6)

if input == output:
        print(anweightdata)
else:
        
        print(input, output)
